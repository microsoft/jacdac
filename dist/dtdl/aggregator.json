    "contents": [
        {
            "@type": "Enum",
            "name": "SampleType",
            "valueSchema": "integer",
            "enumValues": [
                {
                    "name": "U8",
                    "enumValue": 8
                },
                {
                    "name": "I8",
                    "enumValue": 136
                },
                {
                    "name": "U16",
                    "enumValue": 16
                },
                {
                    "name": "I16",
                    "enumValue": 144
                },
                {
                    "name": "U32",
                    "enumValue": 32
                },
                {
                    "name": "I32",
                    "enumValue": 160
                }
            ]
        },
        {
            "@type": "Property",
            "name": "inputs",
            "@id": "dtmi:jacdac:aggregator:80;1",
