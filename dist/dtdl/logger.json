            "name": "log",
            "@id": "dtmi:jacdac:logger:log;1",
            "description": "Report a message."
        },
        {
            "@type": "Unsupportedreport",
            "name": "warn",
            "@id": "dtmi:jacdac:logger:warn;1",
            "description": "Report a message."
        },
        {
            "@type": "Unsupportedreport",
            "name": "error",
            "@id": "dtmi:jacdac:logger:error;1",
            "description": "Report a message."
        }
    ],
